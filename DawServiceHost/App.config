<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="DawServiceHost.TestService">
                <endpoint address="" binding="basicHttpBinding" contract="DawServiceHost.ITestService">
                    <identity>
                        <dns value="localhost" />
                    </identity>
                </endpoint>
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8733/Design_Time_Addresses/DawServiceHost/TestService/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IWorkflowTalkService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:62605/Service1.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IWorkflowTalkService"
                contract="DawWCFServiceRef.IWorkflowTalkService" name="BasicHttpBinding_IWorkflowTalkService" />
        </client>
    </system.serviceModel>
</configuration>